class A
operations
	public op: ()  ==> ()
	op() ==
	(	
		for i = 1 to 100 do
			let - = f[int](100 + i) in skip;
	);
	
	public run: () ==> ()
	run() ==
	(
		start(new A());
		start(new A());
		start(new A());
		op();
	);
		
functions
	public f[@T]: @T -> int
	f(a) == if a = 1 then 1 else fid[@T](-a) + f[@T](a-1)
	measure fid;
	
	public fid[@T]: @T -> nat
	fid(a) == if a < 0 then 1 else a;
	
thread
	op();
	
end A

